
{
    "database_name" : "Check-in",

    "Tables" : [
        {
            "name" : "Reservations",
            "columns" : ["First_name", "Last_name", "id", "Seat", "Phone", 
            "Passport_no", "Gender", "Birth_date", "Flight_id"],
            "primary_key"  : "id",
            "Index_keys" : ["First_name", "Last_name"],
            "Consistency" : "Eventual"
        },
        {
            "name" : "Flights_Details",
            "columns" : ["Flight_id", "Plane_id", "From", "To", "Stops", "Airline name", 
            "Departure time", "Departure date", "Flight duration"],
            "primary_key"  : "Flight_id",
            "Index_keys" : ["From", "To", "Stops", "Airline name"],
            "Consistency" : "Eventual"
        },
        {
            "name" : "Planes_Details",
            "columns" : ["Plane_id", "Plane type", "Seat map"],
            "primary_key"  : "Plane_id",
            "Consistency" : "Eventual"
        },
        {
            "name" : "Flights_seats",
            "columns" : ["Flight_id_seat_no", "id"],
            "primary_key"  : "Flight_id_seat_no",
            "Index_keys" : ["id"],
            "Consistency" : "Strong"
        }
    
    ]
}        
